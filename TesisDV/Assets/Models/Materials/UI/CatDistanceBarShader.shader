				float2 texCoord35 = IN.ase_texcoord3.xy * _Tiling + _Offset;
				float2 panner50 = ( 0.2 * _Time.y * _Speed + texCoord35);
				float4 tex2DNode21 = tex2D( _TextureArrows, ( 1.0 - panner50 ) );
				

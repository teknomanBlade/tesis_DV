			half4 frag ( VertexOutput IN  ) : SV_Target
			{
				UNITY_SETUP_INSTANCE_ID( IN );
				UNITY_SETUP_STEREO_EYE_INDEX_POST_VERTEX( IN );

				#if defined(ASE_NEEDS_FRAG_WORLD_POSITION)
				float3 WorldPosition = IN.worldPos;
				#endif
				float4 ShadowCoords = float4( 0, 0, 0, 0 );

				#if defined(ASE_NEEDS_FRAG_SHADOWCOORDS)
					#if defined(REQUIRES_VERTEX_SHADOW_COORD_INTERPOLATOR)
						ShadowCoords = IN.shadowCoord;
					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				float2 uv_TextureSample0 = IN.ase_texcoord3.xy * _TextureSample0_ST.xy + _TextureSample0_ST.zw;
				float ase_lightAtten = 0;
				Light ase_lightAtten_mainLight = GetMainLight( ShadowCoords );
				ase_lightAtten = ase_lightAtten_mainLight.distanceAttenuation * ase_lightAtten_mainLight.shadowAttenuation;
				float3 normalizeResult60 = normalize( _MainLightPosition.xyz );
				float3 ase_worldNormal = IN.ase_texcoord4.xyz;
				float3 normalizedWorldNormal = normalize( ase_worldNormal );
				float dotResult62 = dot( normalizeResult60 , normalizedWorldNormal );
				float smoothstepResult86 = smoothstep( 0.0 , 0.001 , ( ase_lightAtten * dotResult62 ));
				float3 Normal61 = normalizeResult60;
				float3 ase_worldViewDir = ( _WorldSpaceCameraPos.xyz - WorldPosition );
				ase_worldViewDir = SafeNormalize( ase_worldViewDir );
				float3 normalizeResult57 = normalize( ase_worldViewDir );
				float3 normalizeResult65 = normalize( ( normalizeResult57 + _MainLightPosition.xyz ) );
				float dotResult72 = dot( Normal61 , normalizeResult65 );
				float smoothstepResult88 = smoothstep( 0.001 , 0.01 , pow( ( _MainLightColor.a * dotResult72 ) , ( _Intensity * _Intensity ) ));
				float dotResult70 = dot( ase_worldViewDir , Normal61 );
				float LdotN68 = dotResult62;
				float smoothstepResult87 = smoothstep( ( _RimAmount - 0.01 ) , ( _RimAmount + 0.01 ) , ( ( 1.0 - dotResult70 ) * pow( LdotN68 , _Rim ) ));
				
				float2 uv_MainTexture = IN.ase_texcoord3.xy * _MainTexture_ST.xy + _MainTexture_ST.zw;
				float4 color51 = IsGammaSpace() ? float4(1,1,1,0) : float4(1,1,1,0);
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( tex2D( _TextureSample0, uv_TextureSample0 ) * ( _MainColor * ( ( smoothstepResult86 * _MainLightColor ) + ( _SpecularColor * smoothstepResult88 * ase_lightAtten ) + ( _RimColor * ase_lightAtten * smoothstepResult87 ) + _AmbientColor ) ) ).rgb;
				float Alpha = ( tex2D( _MainTexture, uv_MainTexture ).a * color51 ).r;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON

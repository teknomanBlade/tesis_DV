				float2 uv_TextureSample0 = IN.ase_texcoord3.xy * _TextureSample0_ST.xy + _TextureSample0_ST.zw;
				float3 normalizeResult30 = normalize( _MainLightPosition.xyz );
				float3 ase_worldNormal = IN.ase_texcoord4.xyz;
				float3 normalizedWorldNormal = normalize( ase_worldNormal );
				float dotResult36 = dot( normalizeResult30 , normalizedWorldNormal );
				float smoothstepResult45 = smoothstep( 0.0 , 0.001 , ( 0.0 * dotResult36 ));
				float3 Normal34 = normalizeResult30;
				float3 ase_worldViewDir = ( _WorldSpaceCameraPos.xyz - WorldPosition );
				ase_worldViewDir = SafeNormalize( ase_worldViewDir );
				float3 normalizeResult31 = normalize( ase_worldViewDir );
				float3 normalizeResult38 = normalize( ( normalizeResult31 + _MainLightPosition.xyz ) );
				float dotResult40 = dot( Normal34 , normalizeResult38 );
				float smoothstepResult46 = smoothstep( 0.001 , 0.01 , pow( ( _MainLightColor.a * dotResult40 ) , ( _Intensity * _Intensity ) ));
				float dotResult10 = dot( ase_worldViewDir , Normal34 );
				float LdotN37 = dotResult36;
				float smoothstepResult22 = smoothstep( ( _RimAmount - 0.01 ) , ( _RimAmount + 0.01 ) , ( ( 1.0 - dotResult10 ) * pow( LdotN37 , _Rim ) ));
				
				float2 uv_MainTexture = IN.ase_texcoord3.xy * _MainTexture_ST.xy + _MainTexture_ST.zw;
				float4 color1 = IsGammaSpace() ? float4(1,1,1,0) : float4(1,1,1,0);
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( tex2D( _TextureSample0, uv_TextureSample0 ) * ( _MainColor * ( ( smoothstepResult45 * _MainLightColor ) + ( _SpecularColor * smoothstepResult46 ) + ( _RimColor * float4( 0,0,0,0 ) * smoothstepResult22 ) + _AmbientColor ) ) ).rgb;
				float Alpha = ( tex2D( _MainTexture, uv_MainTexture ).a * color1 ).r;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON
